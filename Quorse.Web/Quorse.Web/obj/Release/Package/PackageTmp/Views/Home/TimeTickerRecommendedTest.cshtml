<div class="panel panel-primary">
    <table class="table table-striped">
        <thead>
            <tr><th>ID</th><th>Title</th><th>Course Price</th><th>CourseEntityName</th><th>Class Highest Price</th><th>Class Lowest Price</th><th></th></tr>
        </thead>
        <tbody data-bind="foreach: model.timeTickerRecommended()">
            <tr>
                <td data-bind="text: $data.CourseId"></td>
                <td data-bind="text: $data.CourseTitle"></td>
                <td data-bind="text: $data.CoursePrice.toFixed(2)"></td>
                <td data-bind="text: $data.CourseEntityName"></td>
                <td data-bind="text: $data.ClassSimpleModel.CourseHighestClassPrice.toFixed(2)"></td>
                <td data-bind="text: $data.ClassSimpleModel.CourseLowestClassPrice.toFixed(2)"></td>
            </tr>
        </tbody>
    </table>
</div>
